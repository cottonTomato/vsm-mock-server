{
  "name": "vsm-mock-server",
  "version": "1.0.0",
  "description": "Mock server for vsm frontend testing",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "start": "tsx src/app.ts",
    "start:dev": "tsx watch --ignore src/**/*.spec.ts --ignore e2e --clear-screen=false src/app.ts",
    "start:prod": "tsc && node dist/app.js"
  },
  "keywords": [],
  "author": "cottonTomato",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "prettier": "^3.0.3",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2"
  }
}
